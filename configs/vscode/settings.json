{
	"window.restoreWindows": "none",
	"workbench.startupEditor": "none",
	"workbench.editor.highlightModifiedTabs": true,
	"terminal.integrated.fontFamily": "MesloLGS NF",
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "line",
	"explorer.sortOrder": "type",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"liveServer.settings.donotShowInfoMsg": true,
	"emmet.includeLanguages": {
		"erb": "html",
		"javascript": "javascriptreact",
		"typescript": "typescriptreact"
	},

	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"javascript.suggest.autoImports": true,
	"typescript.suggest.autoImports": true,

	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.renderControlCharacters": true,
	"editor.rulers": [110],
	"editor.showFoldingControls": "mouseover",
	"editor.quickSuggestions": {
		"other": "on",
		"comments": "off",
		"strings": true
	},

	"files.autoSave": "onFocusChange",
	"files.trimTrailingWhitespace": true,
	"files.hotExit": "onExit",
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"**/*.*.meta": true,
		"**/*.tga": true,
		"**/*.cubemap": true,
		"**/Temp": true,
		"**/*.exe": true,
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true,
		"**/.vscode": true,
		"**/*.meta": true,
		"**/Icon*": true
	},
	"editor.tabSize": 3,
	"editor.insertSpaces": true,
	"editor.detectIndentation": false,
	"editor.fontFamily": "'JetBrains Mono', 'MesloLGS NF'",
	"editor.fontLigatures": true,
	"editor.smoothScrolling": true,
	"editor.cursorBlinking": "smooth",
	"editor.cursorStyle": "line",
	"editor.minimap.enabled": false,
	"editor.wordWrap": "wordWrapColumn",
	"editor.wordWrapColumn": 100,
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.unicodeHighlight.nonBasicASCII": false,
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"prettier.jsxSingleQuote": true,
	"prettier.printWidth": 100,
	"prettier.tabWidth": 3,
	"prettier.semi": true,
	"prettier.singleQuote": true,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"prettier.endOfLine": "auto",
	"editor.codeLens": false,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.sortImport": "explicit",
		"source.organizeImports": "explicit"
	},

	// "code-runner.executorMap": {
	// 	"c": "cd $dir && gcc *.c -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
	// },

	"editor.inlineSuggest.enabled": true,
	"[dart]": {
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.rulers": [80],
		"editor.selectionHighlight": false,
		"editor.suggest.snippetsPreventQuickSuggestions": true,
		"editor.suggestSelection": "recentlyUsedByPrefix",
		"editor.tabCompletion": "onlySnippets",
		"editor.wordBasedSuggestions": "off"
	},
	"editor.foldingStrategy": "indentation",
	"workbench.list.smoothScrolling": true,
	"git.enabled": true,
	"editor.fontSize": 13,
	"window.newWindowDimensions": "maximized",
	"window.commandCenter": false,
	"workbench.productIconTheme": "icons-carbon",
	"workbench.colorCustomizations": {
		"input.background": "#111111",
		"list.activeSelectionBackground": "#111111",
		"list.hoverBackground": "#111111",
		"editorWidget.background": "#111111",
		"panel.background": "#111111",
		"titleBar.activeBackground": "#0B0B0C",
		"titleBar.inactiveBackground": "#0B0B0C",
		"breadcrumbPicker.background": "#111111",
		"settings.dropdownBackground": "#111111",
		"settings.numberInputBackground": "#111111",
		"settings.textInputBackground": "#111111",
		"settings.checkboxBackground": "#111111",
		"editor.background": "#000000",
		"tab.activeBackground": "#0B0B0C",
		"tab.inactiveBackground": "#000000",
		"sideBar.background": "#0B0B0C",
		"sideBarSectionHeader.background": "#0B0B0C", //1a1a1a
		"activityBar.background": "#0B0B0C",
		"statusBar.background": "#0B0B0C",
		"scrollbarSlider.activeBackground": "#0B0B0C",
		"tab.activeBorder": "#000000",
		"editorGroupHeader.tabsBackground": "#000000",
		"statusBar.noFolderBackground": "#000000",
		"tab.border": "#000000",
		"editorHoverWidget.background": "#000000",
		"menu.background": "#000000",
		"debugToolBar.background": "#000000",
		"editorBracketMatch.background": "#000000",
		"quickInput.background": "#000000",
		"editorOverviewRuler.border": "#000000",
		"notifications.background": "#000000",
		"breadcrumb.background": "#000000",
		"editorIndentGuide.activeBackground1": "#9CDBFB"
	},
	"workbench.iconTheme": "icons",
	"extensions.ignoreRecommendations": true,
	"diffEditor.hideUnchangedRegions.enabled": true,
	"git.openRepositoryInParentFolders": "never",
	"editor.guides.highlightActiveIndentation": true,
	"editor.guides.bracketPairsHorizontal": "active",
	"editor.acceptSuggestionOnCommitCharacter": true,
	"editor.accessibilitySupport": "off",
	"diffEditor.ignoreTrimWhitespace": false,
	"workbench.editor.enablePreview": false,
	"[latex]": {
		"editor.defaultFormatter": "James-Yu.latex-workshop",
		"editor.formatOnSave": false
	},
	"update.showReleaseNotes": false,
	"latex-workshop.latex.tools": [
		{
			"name": "latexmk",
			"command": "latexmk",
			"args": ["-xelatex", "-synctex=1", "-interaction=nonstopmode", "-file-line-error", "%DOC%"]
		}
	],
	"editor.foldingImportsByDefault": true,
	"workbench.colorTheme": "Material Theme High Contrast"
}
